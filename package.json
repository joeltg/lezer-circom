{
	"name": "lezer-circom",
	"version": "0.0.4",
	"description": "A Lezer grammar for the Circom language",
	"type": "module",
	"main": "index.js",
	"types": "index.d.ts",
	"files": [
		"index.js",
		"index.d.ts",
		"circom.js",
		"circom.d.ts",
		"circom.terms.js",
		"circom.terms.d.ts"
	],
	"scripts": {
		"test": "ava",
		"build": "npm run generate-parser-types && npm run generate-term-types",
		"generate-parser-types": "tsc --allowJs --declaration --emitDeclarationOnly index.js",
		"generate-term-types": "tsc --allowJs --declaration --emitDeclarationOnly circom.terms.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/joeltg/lezer-circom.git"
	},
	"keywords": [
		"circom",
		"lezer"
	],
	"author": "Joel Gustafson",
	"license": "GPLv3",
	"bugs": {
		"url": "https://github.com/joeltg/lezer-circom/issues"
	},
	"homepage": "https://github.com/joeltg/lezer-circom#readme",
	"dependencies": {
		"@lezer/common": "^0.15.7",
		"@lezer/lr": "^0.15.4"
	},
	"devDependencies": {
		"@lezer/generator": "^0.15.2",
		"ava": "^4.0.0",
		"typescript": "^4.5.5"
	}
}
